{
  "name": "logseq-plugin-runjs",
  "version": "0.1.0",
  "description": "Run arbitrary JS code in Logseq",
  "scripts": {
    "dev": "parcel serve ./index.html --public-url ./ --no-hmr",
    "build": "parcel build ./index.html --public-url ."
  },
  "repository": "https://github.com/meain/logseq-plugin-runjs.git",
  "keywords": [],
  "author": "Abin Simon",
  "license": "Apache-2.0",
  "private": false,
  "logseq": {
    "main": "dist/index.html",
    "icon": "./icon.png",
    "id": "logseq-plugin-runjs",
    "title": "Run JS"
  },
  "dependencies": {
    "@logseq/libs": "^0.0.10",
    "crypto-es": "^2.1.0",
    "react": "17.0.2"
  },
  "alias": {
    "react/jsx-dev-runtime": "./node_modules/react/jsx-dev-runtime.js",
    "react/jsx-runtime": "./node_modules/react/jsx-runtime.js",
    "react": "./src/React.ts",
    "react-dom": "./src/ReactDOM.ts"
  },
  "devDependencies": {
    "@types/react": "^17.0.39",
    "parcel": "latest"
  }
}
